#!/bin/bash

##
## initialization which needs to take place inside the container
##

usage() {
        echo "usage: init" 2>&1
        exit 1
}

cd /aoc/src || exit 1

cargo add itertools lexer maplit petgraph regex unicode-segmentation

## I'd rather do this in Dockerfile, but it didn't seem to work that way
cargo install cargo-expand cargo-show-asm irust
