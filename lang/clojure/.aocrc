# code: language=shellscript insertSpaces=true tabSize=2
# shellcheck shell=bash

already_initialized() {
        if [[ -e "years/${AOC_YEAR}/clojure/project.clj" ]]; then
                return 0
        else
                return 1
        fi
}
declare -x already_initialized

if [[ -n "${AOC_YEAR}" ]]; then
        export ADDITIONAL_MOUNTS="\
--mount type=volume,src=aoc-clojure-${AOC_YEAR}-m2,dst=/home/aoc/.m2 \
--mount type=volume,src=aoc-clojure-${AOC_YEAR}-target,dst=/aoc/src/target \
"
fi
