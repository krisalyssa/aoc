# code: language=shellscript insertSpaces=true tabSize=2
# shellcheck shell=bash

already_initialized() {
        if [[ -e "years/${AOC_YEAR}/typescript/package.json" ]]; then
                return 0
        else
                return 1
        fi
}
declare -x already_initialized

if [[ -n "${AOC_YEAR}" ]]; then
        export ADDITIONAL_MOUNTS="\
--mount type=volume,src=aoc-typescript-${AOC_YEAR}-cache,dst=/home/aoc/.cache \
--mount type=volume,src=aoc-typescript-${AOC_YEAR}-yarn,dst=/aoc/src/.yarn \
"
fi
