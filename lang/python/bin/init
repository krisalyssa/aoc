#!/bin/bash

##
## initialization which needs to take place inside the container
##

usage() {
        echo "usage: init" 2>&1
        exit 1
}

cd /aoc/src || exit 1

[[ -e /aoc/src/setup.py.skel ]] && [[ ! -e /aoc/src/setup.py ]] && mv -v /aoc/src/setup.py.skel /aoc/src/setup.py
[[ -e /aoc/src/aoc/__init__.py.skel ]] && [[ ! -e /aoc/src/aoc/__init__.py ]] && mv -v /aoc/src/aoc/__init__.py.skel /aoc/src/aoc/__init__.py
[[ -e /aoc/src/tests/__init__.py.skel ]] && [[ ! -e /aoc/src/tests/__init__.py ]] && mv -v /aoc/src/tests/__init__.py.skel /aoc/src/tests/__init__.py
