# code: language=shellscript insertSpaces=true tabSize=2
# shellcheck shell=bash

already_initialized() {
        if [[ -e "years/${AOC_YEAR}/gleam/gleam.toml" ]]; then
                return 0
        else
                return 1
        fi
}
declare -x already_initialized

if [[ -n "${AOC_YEAR}" ]]; then
        export ADDITIONAL_MOUNTS="\
--mount type=volume,src=aoc-gleam-${AOC_YEAR}-build,dst=/aoc/src/build \
"
fi
